{
  "name": "vscode-pwa-analyzer",
  "version": "1.0.0",
  "description": "Log viewer for https://github.com/microsoft/vscode-pwa",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "fmt": "prettier --write \"src/**/*.{ts,tsx,scss}\" && npm run lint -- --fix",
    "start": "webpack-dev-server --open",
    "build": "webpack --config webpack.production.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/vscode-pwa-analyzer.git"
  },
  "author": "Connor Peet <connor@peet.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/vscode-pwa-analyzer/issues"
  },
  "homepage": "https://github.com/microsoft/vscode-pwa-analyzer#readme",
  "devDependencies": {
    "@types/lodash-es": "4.17.10",
    "@types/pako": "2.0.2",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "bootstrap": "5.3.2",
    "css-loader": "6.8.1",
    "html-webpack-plugin": "5.5.3",
    "node-sass": "9.0.0",
    "normalize.css": "8.0.1",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "react-data-grid": "7.0.0-beta.40",
    "react-dom": "18.2.0",
    "react-json-view": "1.21.3",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "ts-loader": "9.5.0",
    "typescript": "5.2.2",
    "use-debounce": "9.0.4",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "avoid",
    "printWidth": 100,
    "tabWidth": 2
  },
  "dependencies": {
    "lodash-es": "4.17.21",
    "pako": "2.1.0"
  }
