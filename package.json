{
	"name": "vscode-pwa-analyzer",
	"version": "1.0.0",
	"description": "Log viewer for https://github.com/microsoft/vscode-pwa",
	"homepage": "https://github.com/microsoft/vscode-pwa-analyzer#readme",
	"bugs": {
		"url": "https://github.com/microsoft/vscode-pwa-analyzer/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/microsoft/vscode-pwa-analyzer.git"
	},
	"license": "MIT",
	"author": "Connor Peet <connor@peet.io>",
	"main": "index.js",
	"scripts": {
		"build": "webpack --config webpack.production.js",
		"fmt": "prettier --write \"src/**/*.{ts,tsx,scss}\" && npm run lint -- --fix",
		"start": "webpack-dev-server --open",
		"test": "npm run lint"
	},
	"prettier": {
		"arrowParens": "avoid",
		"printWidth": 100,
		"singleQuote": true,
		"tabWidth": 2,
		"trailingComma": "all"
	},
	"dependencies": {
		"lodash-es": "4.17.21",
		"pako": "2.1.0"
	},
	"devDependencies": {
		"@types/lodash-es": "4.17.11",
		"@types/pako": "2.0.3",
		"@types/react": "18.2.37",
		"@types/react-dom": "18.2.15",
		"bootstrap": "5.3.2",
		"css-loader": "6.8.1",
		"html-webpack-plugin": "5.5.3",
		"node-sass": "9.0.0",
		"normalize.css": "8.0.1",
		"react": "18.2.0",
		"react-data-grid": "7.0.0-beta.41",
		"react-dom": "18.2.0",
		"react-json-view": "1.21.3",
		"sass-loader": "13.3.2",
		"style-loader": "3.3.3",
		"ts-loader": "9.5.1",
		"typescript": "5.2.2",
		"use-debounce": "10.0.0",
		"webpack": "5.89.0",
		"webpack-cli": "5.1.4",
		"webpack-dev-server": "4.15.1"
	}
}
