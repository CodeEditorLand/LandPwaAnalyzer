{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "declaration": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "moduleResolution": "node",
    "newLine": "LF",
    "jsx": "react",
    "module": "esnext",
    "target": "ES2023",
    "lib": ["ES2023", "dom"],
    "outDir": "dist",
  }
}
